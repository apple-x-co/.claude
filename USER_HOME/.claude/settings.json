{
    "permissions": {
        "deny": [
            "Bash (sudo: *)",
            "Bash (rm -rf:*)",
            "Read (.env*)",
            "Read (id_rsa)",
            "Read (id_ed25519)",
            "Read (**/*key*)",
            "Read (**/- ssh/**)",
            "Write(.env*)",
            "Write（**/secrets/**）",
            "Write（**/-ssh/**）",
            "Bash（npmuninstall：*）",
            "Bash（npmremove：*＊）",
            "Bash (psql:*)",
            "Bash (mysql:*)",
            "Bash (mongod:*)",
            "NotebookEdit"
        ],
        "ask": [
            "Bash (rm: *) ",
            "Bash (curl:*)",
            "Bash（chmod+X：＊）",
            "Bash（gitcommit：*）",
            "Bash（gitbranch：*）",
            "Bash（gitmerge：*）",
            "Bash（gitstash：*）",
            "Bash（gitfetch：*）",
            "Bash（gitpush：*）",
            "Bash(git pull:*)",
            "Bash（gitreset：*）",
            "Bash（gitrebase：*）",
            "Bash(git tag:*)",
            "Bash（npmpublish：*）"
        ]
    },
    "model": "sonnet",
    "statusLine": {
        "type": "command",
        "command": "/bin/bash ~/.claude/statusline-command.sh"
    }
}
